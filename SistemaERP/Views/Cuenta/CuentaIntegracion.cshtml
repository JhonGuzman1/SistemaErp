            let CreditoFiscal=  $("#select_CreditoFiscal").val();
            let DebitoFiscal = $("#select_DebitoFiscal").val();
            let Compras = $("#select_Compras").val();
            let Ventas = $("#select_Ventas").val();
            let It = $("#select_It").val();
            let ItPagar= $("#select_ItPagar").val();

            if (Caja == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta caja");
            }

            if (CreditoFiscal == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta credito fiscal ");
            }

            if (DebitoFiscal == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta debito fiscal");
            }

            if (Compras == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta compras");
            }

            if (Ventas == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta ventas");
            }

            if (It == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta it");
            }

            if (ItPagar == -1) {
                return MostrarMensaje("Debe seleccionar la cuenta it por pagar");
            }

          
                $.ajax({
                    url: "@Url.Action("RegistroCuentaIntegracion", "Cuenta")",
